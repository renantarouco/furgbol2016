#include "Tatica.h"
#include <Sistema.h>

Tatica::Tatica(){
}

Tatica::~Tatica(){
}

void Tatica::addParamInt(int param){
    paramInt.push_back(param);
}

void Tatica::addParamFloat(float param){
    paramFloat.push_back(param);
}

void Tatica::addParamPonto(Ponto ponto){
    paramPonto.push_back(ponto);
}

void Tatica::addParamString(string param){
    paramString.push_back(param);
}

void Tatica::addParamBool(bool param){
    paramBool.push_back(param);
}

void Tatica::setParamPonto(const vector<Ponto>& paramPonto){
    this->paramPonto = paramPonto;
}

void Tatica::setParamString(vector<string> _paramString){
    paramString = _paramString;
}

void Tatica::setParamInt(vector<int> _paramInt){
    paramInt = _paramInt;
}

vector<Ponto> Tatica::getParamPonto(){
    return paramPonto;
}

vector<std::string> Tatica::getParamString(){
    return paramString;
}

vector<int> Tatica::getParamInt(){
    return paramInt;
}

vector<float> Tatica::getParamFloat(){
    return paramFloat;
}

vector<bool> Tatica::getParamBool(){
    return paramBool;
}

void Tatica::setParamBool(vector<bool> _paramBool)
{
    paramBool = _paramBool;
}

void Tatica::setParamFloat(vector<float> _paramFloat){
    paramFloat = _paramFloat;
}
